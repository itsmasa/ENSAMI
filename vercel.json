{
    "builds": [
      {
        "src": "chatbot.py",
        "use": "@vercel/python",
        "install": "pip install -r requirements.txt"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "chatbot.py"
      }
    ],
    "build": {
      "env": {
        "PIP_INSTALL": "--requirement=requirements.txt"
      }
    }
    
  }